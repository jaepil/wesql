+  std::string archived_binlog{};
+  my_off_t mysql_binlog_pos = 0;
+  my_off_t mysql_binlog_write_pos = 0;
+  my_off_t archived_binlog_pos = 0;
+  my_off_t archived_binlog_write_pos = 0;
+  uint64_t consensus_index = 0;
